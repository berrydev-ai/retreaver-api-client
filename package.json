{
  "name": "retreaver-api-client",
  "version": "1.0.0",
  "description": "TypeScript/JavaScript client for the Retreaver Core API - manage call tracking, routing, affiliates, campaigns, and more",
  "author": "Retreaver",
  "repository": {
    "type": "git",
    "url": "https://github.com/berrydev-ai/retreaver-api-client.git"
  },
  "keywords": [
    "retreaver",
    "call-tracking",
    "call-routing",
    "api-client",
    "axios",
    "typescript",
    "openapi-client"
  ],
  "license": "MIT",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "module": "./dist/esm/index.js",
  "sideEffects": false,
  "scripts": {
    "build": "tsc && tsc -p tsconfig.esm.json",
    "prepare": "npm run build"
  },
  "dependencies": {
    "axios": "^1.6.1"
  },
  "devDependencies": {
    "@types/node": "12.11.5 - 12.20.42",
    "typescript": "^4.0 || ^5.0"
